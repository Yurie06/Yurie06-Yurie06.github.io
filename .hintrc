{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "width: fit-content",
          "-webkit-overflow-scrolling",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 107",
    "not ios_saf <= 16.2",
    "not safari <= 16.2"
  ]
}